{
	"easycom": {
		"autoscan": true,
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue",
		"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
		"^x-(.*)": "@/components/x-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		}, {
			"path": "pages/second/second",
			"style": {
				"navigationBarTitleText": "second",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/third/third",
			"style": {
				"navigationBarTitleText": "third",
				"enablePullDownRefresh": false
			}

		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {},
	"tabBar": {
		"color": "#333333",
		"selectedColor": "#FD2E22",
		"borderStyle": "white",
		"backgroundColor": "#FFFFFF",
		"height": "50px",
		"fontSize": "10px",
		"iconWidth": "30px",
		"spacing": "0",
		"list": [{
			"pagePath": "pages/index/index",
			"iconPath": "static/c1.png",
			"selectedIconPath": "static/c1.png",
			"dark-iconPath": "static/c4.png",
			"dark-selectedIconPath": "static/c4.png",
			"text": "首页"
		}, {
			"pagePath": "pages/second/second",
			"iconPath": "static/c2.png",
			"selectedIconPath": "static/c2.png",
			"dark-iconPath": "static/c5.png",
			"dark-selectedIconPath": "static/c5.png",
			"text": "second"
		}, {
			"pagePath": "pages/third/third",
			"iconPath": "static/c3.png",
			"selectedIconPath": "static/c3.png",
			"dark-iconPath": "static/c6.png",
			"dark-selectedIconPath": "static/c6.png",
			"text": "third"
		}]
	}
}
